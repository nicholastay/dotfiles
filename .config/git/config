[alias]
	c = commit
	p = push
	s = status
	su = status --untracked-files=normal
	sua = status -u
	rv = remote -v
	a = add
	aa = add .
	co = checkout
	cam = commit -a -m
	cm = commit -m
	d = diff
	cob = checkout -b
	l = log
[include]
	path = ~/.config/git/user
[credential]
	helper = /usr/lib/git-core/git-credential-libsecret
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
[pull]
	rebase = true
